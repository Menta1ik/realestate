{
  "name": "tg-realestate-webapp-mvp-v2",
  "private": true,
  "version": "0.2.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "ls -R dist-server && node dist-server/backend/src/main.js",
    "start:static": "serve -s dist -l ${PORT:-3000}",
    "start:prod": "ls -R dist-server && node dist-server/backend/src/main.js",
    "preview": "vite preview",
    "postinstall": "prisma generate --schema=backend/prisma/schema.prisma",
    "db:push": "prisma db push --schema=backend/prisma/schema.prisma",
    "db:seed": "ts-node backend/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.22",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^10.4.22",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^10.4.22",
    "@nestjs/serve-static": "^4.0.2",
    "@prisma/client": "^6.19.2",
    "axios": "^1.13.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "serve": "^14.2.5",
    "swiper": "^12.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^17.2.4",
    "prisma": "^6.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.2"
  }
}
